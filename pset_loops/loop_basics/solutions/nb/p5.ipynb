    "\"\"\"\n",
    "Factors - SOLUTION\n",
    "\"\"\"\n",
    "\n",
    "# Find all factors of a number that a user inputs.\n",
    "\n",
    "user_input = input(\"Enter a number to find its factors: \")\n",
    "a = int(user_input)\n",
    "\n",
    "print(f'The factors of {a} are: ')\n",
    "\n",
    "for i in range(1, a + 1):\n",
    "  if a % i == 0:\n",
    "    print(i)"
   ]
